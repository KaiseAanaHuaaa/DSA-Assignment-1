   "source": [
    "def reverse_list(A, start, end):\n",
    "    while start < end:\n",
    "        A[start], A[end] = A[end], A[start]\n",
    "        start += 1\n",
    "        end -= 1\n",
    "A = [1, 2, 3, 4, 5, 6]\n",
    "print(A)\n",
    "reverse_list(A, 0, 5)\n",
    "print(\"Reversed list is\")\n",
    "print(A)"
   ]
  }
